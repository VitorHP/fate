%span.character__sheet-title= t("activerecord.models.consequence", count: 2)
%ul.list-unstyled.character__skills
  - [:mild, :moderate, :severe].each do |level|
    - consequences.send(level).each do |c|
      %li
        .row
          .col-xs-3
            %span.character__stress-track-label= "#{t("activerecord.attributes.consequence.#{level}")} (#{c.level})"
          .col-xs-9
            %span= c.description
